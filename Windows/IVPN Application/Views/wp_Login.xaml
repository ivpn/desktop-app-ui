    </Page.Resources>

    <StackPanel Margin="23,25,23,24">

        <Image Height="29"  Source="{StaticResource logo}"  SnapsToDevicePixels="True"/>
        <!--<Image Height="42" Source="/IVPN Client;component/Resources/logoIvpn.png" SnapsToDevicePixels="True"/>-->

        <Image Width="177" Margin="0,34,0,29" Source="{StaticResource imageLaptopDark}"  SnapsToDevicePixels="True"/>
        <!--<Image Stretch="None" Margin="0,27,0,29" Source="/IVPN Client;component/Resources/imageLaptopDark.png" SnapsToDevicePixels="True"/>-->

        <Popup x:Name="GuiWrongPasswordPopup" StaysOpen="False" Placement="Top" 
               MaxWidth="300"
               HorizontalOffset="10" VerticalOffset="6" 
               PlacementTarget="{Binding ElementName= GuiUsernamePlaceholder}" PopupAnimation="Fade" AllowsTransparency="True">
            <Border BorderThickness="0" Background="#ff2f38" Opacity="0.99">
                <TextBlock FontSize="13" Foreground="White" Margin="8" Name="GuiTextBlockWrongPassword" TextWrapping="Wrap">
                    Account ID you entered was incorrect. Please try again.
                </TextBlock>
            </Border>
        </Popup>

        <Grid Margin="0,0,0,13">
            <Label Style="{StaticResource LabelPlaeholder}" Content="i-XXXX-... or ivpnXXXXXXXX" Name="GuiUsernamePlaceholder"
			    Width="{Binding Width, ElementName=GuiTextBoxUsername}"
			    Height="{Binding Height, ElementName=GuiTextBoxUsername}"/>
            <TextBox Style="{StaticResource TextBoxDefault}" x:Name="GuiTextBoxUsername"
			    HorizontalAlignment="Stretch"
			    Height="36" 
			    Text="{Binding ViewModel.UserName, UpdateSourceTrigger=PropertyChanged}"/>
        </Grid>
        
        <Button Name="GuiButtonLogin"
                Background="#178FE6"
                Height="42" 
                HorizontalAlignment="Stretch"
                BorderThickness="0"
                Click="GuiButtonLogin_OnClick" IsDefault="True"
                Style="{StaticResource ButtonMouseOverAndPressedStyle}">
            <TextBlock FontSize="15" Foreground="White" Name="LoginBtnCaption">Log In</TextBlock>
        </Button>

        <StackPanel Orientation="Vertical" MinHeight="14">
            <ProgressBar Name="GuiProgressBar" Margin="0,7,0,0"
                     HorizontalAlignment="Stretch" 
                     Width="272" IsIndeterminate="True"
                     />
        </StackPanel>
        
        <TextBlock HorizontalAlignment="Stretch" TextWrapping="Wrap" TextAlignment="Center" 
                   Foreground="#FF98AABA" 
                   FontSize="12" TextOptions.TextFormattingMode="Display" >
            Your account ID can be found in the 
            <Hyperlink Click="Hyperlink_ClientAreaClick" >Client Area</Hyperlink> 
            of the website
        </TextBlock>
        
        <Button Name="GuiButtonStartFreeTrial" Margin="0,45,0,0"
                Background="#FFC9D0D7"
                Height="42" 
                HorizontalAlignment="Stretch"
                BorderThickness="0"
                Click="GuiButtonStartFreeTrial_OnClick"
                Style="{StaticResource ButtonMouseOverAndPressedStyle}">
            
            <TextBlock FontSize="15" Foreground="#ff385777">Start my free trial</TextBlock>
        </Button>
        
    </StackPanel>
